{
  "name": "sveltekit-web",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite build && vite preview --port $(microfrontends port)",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-vercel": "^5.5.3",
    "@sveltejs/kit": "^2.17.2",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@vercel/microfrontends": "workspace:*",
    "svelte": "^5.20.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "eslint-config-custom": "workspace:*",
    "postcss": "^8.4.47",
    "svelte-check": "^4.1.4",
    "tailwindcss": "^3.4.14",
    "ts-config": "workspace:*",
    "typescript": "5.7.3",
    "vite": "5.4.11"
  }
}
